{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "array",
    "items": {
      "type": "object",
      "additionalProperties": false,
      "required": ["displayName", "twitch", "discord", "discordProfileUrl", "score"],
      "properties": {
        "displayName": {
          "type": "string"
        },
        "pronouns": {
          "type": "string"
        },
        "twitch": {
          "type": "string"
        },
        "discord": {
          "type": "string"
        },
        "discordProfileUrl": {
          "type": "string"
        },
        "country": {
          "type": "string"
        },
        "score": {
          "type": "number",
          "default": 0
        }
      }
    }
  }
  