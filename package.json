{
  "name": "bingothon-layouts",
  "version": "1.0.0",
  "description": "Layouts for Bingothon using the template for NodeCG bundles that use Vue.js and TypeScript.",
  "homepage": "https://github.com/bingothon/bingothon-layouts",
  "bugs": {
    "url": "https://github.com/bingothon/bingothon-layouts/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bingothon/bingothon-layouts.git"
  },
  "license": "MIT",
  "author": "Bingothon",
  "contributors": [
    "lepelog",
    "floha258"
  ],
  "engines": {
    "npm": ">=8.0.0 <9.0.0",
    "node": ">=16.0.0 <17.0.0"
  },
  "scripts": {
    "autofix": "run-s autofix:*",
    "autofix:browser": "eslint --fix --ext .ts,.vue src/dashboard && eslint --fix --ext .ts,.vue src/graphics && eslint --fix --ext .ts src/browser-util",
    "autofix:extension": "eslint --fix --ext .ts src/extension && eslint --fix --ext .d.ts src/types",
    "build": "run-s build:*",
    "build:browser": "cross-env NODE_ENV=production webpack",
    "build:extension": "tsc -b tsconfig.extension.json",
    "clean": "trash node_modules/.cache && trash dashboard && trash graphics && trash extension",
    "start": "node ../..",
    "typeschemas": "node script/typeschemas.js",
    "watch": "run-p watch:*",
    "watch:browser": "webpack -w",
    "watch:extension": "tsc -b tsconfig.extension.json -w",
    "tscver": "tsc --version",
    "lint": "run-s lint:*",
    "lint:browser": "eslint --ext .ts,.js,.html,.vue src/dashboard src/graphics src/browser-util",
    "lint:extension": "eslint --ext .ts,.js,.html src/extension types"
  },
  "devDependencies": {
    "@mdi/font": "^5.4.55",
    "@types/cheerio": "^0.22.13",
    "@types/clone": "^0.1.30",
    "@types/d3": "^5.7.2",
    "@types/deep-equal": "^1.0.1",
    "@types/node": "^14.0.27",
    "@types/request-promise": "^4.1.44",
    "@types/tmi.js": "^1.7.1",
    "@types/webpack-env": "^1.17.0",
    "@types/ws": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "@vue/component-compiler-utils": "^3.3.0",
    "clone": "^2.1.2",
    "cross-env": "^7.0.2",
    "css-loader": "^6.7.1",
    "deepmerge": "^4.2.2",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.7.0",
    "file-loader": "^6.0.0",
    "fork-ts-checker-webpack-plugin": "^7.2.11",
    "globby": "^11.0.1",
    "html-webpack-plugin": "^5.5.0",
    "json-schema-to-typescript": "^11.0.2",
    "mini-css-extract-plugin": "^2.6.0",
    "moment": "^2.29.1",
    "nodecg": "^1.7.0",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.7.1",
    "trash-cli": "^3.1.0",
    "ts-loader": "^8.0.2",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "tslib": "^2.0.0",
    "typescript": "^4.7.3",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.5",
    "vue-hot-reload-api": "^2.3.4",
    "vue-loader": "^15.9.3",
    "vue-property-decorator": "^9.0.0",
    "vue-router": "^3.1.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "vuetify": "^2.6.10",
    "vuetify-loader": "^1.6.0",
    "vuex": "^3.5.1",
    "vuex-class": "^0.3.2",
    "vuex-class-state2way": "^1.0.1",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.9.2",
    "webpack-livereload-plugin": "^3.0.2"
  },
  "alias": {
    "vue": "./node_modules/vue/dist/vue.esm.js"
  },
  "nodecg": {
    "compatibleRange": "^1.5.0",
    "bundleDependencies": {
      "nodecg-speedcontrol": "^2.0.0"
    },
    "dashboardPanels": [
      {
        "name": "bingo-control",
        "title": "Bingo Control",
        "file": "bingo-control.html",
        "headerColor": "#b92b36",
        "width": 2
      },
      {
        "name": "bingomode-control",
        "title": "Bingo Mode Control",
        "file": "bingomode-control.html",
        "headerColor": "#b92b36",
        "width": 2
      },
      {
        "name": "camnames-control",
        "title": "Cam Names Control",
        "file": "camnames-control.html",
        "headerColor": "#b92b36",
        "width": 2,
        "workspace": "Misc"
      },
      {
        "name": "layout-control",
        "title": "Layout Control",
        "file": "layout-control.html",
        "headerColor": "#b92b36",
        "width": 2
      },
      {
        "name": "twitch-control",
        "title": "Twitch Control",
        "file": "twitch-control.html",
        "headerColor": "#b92b36",
        "width": 2
      },
      {
        "name": "obs-control",
        "title": "OBS Control",
        "file": "obs-control.html",
        "headerColor": "#b92b36",
        "width": 2
      },
      {
        "name": "checklist",
        "title": "Checklist",
        "file": "checklist.html",
        "headerColor": "#b92b36",
        "width": 4
      },
      {
        "name": "twitch-resizer-dashboard",
        "title": "Twitch Resizer",
        "width": 10,
        "file": "crop-control.html",
        "headerColor": "#008BB9",
        "workspace": "Twitch Resizing"
      },
      {
        "name": "chat-bot-control",
        "title": "Twitch Chat Bot Control",
        "file": "chat-bot-control.html",
        "headerColor": "#b92b36",
        "width": 2,
        "workspace": "Misc"
      },
      {
        "name": "omnibarMessage-control",
        "title": "Omnibar Message Control",
        "file": "omnibarMessage-control.html",
        "headerColor": "#b92b36",
        "width": 4,
        "workspace": "Misc"
      },
      {
        "name": "tracker-control",
        "title": "Tracker Control",
        "file": "tracker-control.html",
        "headerColor": "#b92b36",
        "width": 2,
        "workspace": "Misc"
      },
      {
        "name": "bestOfX-control",
        "title": "Best Of X Control",
        "file": "bestOfX-control.html",
        "headerColor": "#b92b36",
        "width": 2,
        "workspace": "Misc"
      }
    ],
    "graphics": [
      {
        "file": "game-layout.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "intermission.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "host-dashboard.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "exploration-bingo.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "interview.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "charity-interview.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "cam-names.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "video-player.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "omnibar.html",
        "width": 1920,
        "height": 80
      }
    ],
    "mount": [
      {
        "directory": "static",
        "endpoint": "static"
      }
    ],
    "assetCategories": [
      {
        "name": "charityVideos",
        "title": "Charity Videos",
        "allowedTypes": [
          "mp4"
        ]
      },
      {
        "name": "sponsorVideos",
        "title": "Sponsor Videos",
        "allowedTypes": [
          "mp4"
        ]
      },
      {
        "name": "wideLargeLogos",
        "title": "Wide Large Logos",
        "allowedTypes": [
          "png",
          "jpg",
          "jpeg"
        ]
      },
      {
        "name": "wideSmallLogos",
        "title": "Wide Small Logos",
        "allowedTypes": [
          "png",
          "jpg",
          "jpeg"
        ]
      },
      {
        "name": "squareLogos",
        "title": "Square Logos",
        "allowedTypes": [
          "png",
          "jpg",
          "jpeg"
        ]
      }
    ]
  },
  "vue": {
    "runtimeCompiler": true
  },
  "dependencies": {
    "@discordjs/opus": "^0.5.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.22",
