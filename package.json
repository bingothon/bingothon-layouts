    "cheerio": "^1.0.0-rc.3",
    "chokidar": "^3.5.1",
    "copy-webpack-plugin": "^6.3.1",
    "css-loader": "^5.0.0",
    "d3": "^3.5.17",
    "deep-equal": "^1.0.1",
    "discord.js": "^12.5.3",
    "firebase": "^8.7.0",
    "firebase-app": "^0.0.1",
    "firebase-auth": "^0.1.2",
    "gsap": "^3.6.0",
    "moment": "^2.29.1",
    "mpc-js": "^1.2.1",
    "obs-websocket-js": "^5.0.2",
    "request-promise": "^4.2.4",
    "sass": "^1.52.2",
    "sass-loader": "^13.0.0",
    "tmi.js": "^1.7.1",
    "ts-loader": "^8.0.7",
    "vuefire": "^2.2.5",
    "vuetify": "^2.6.10",
    "vuetify-loader": "^1.6.0",
