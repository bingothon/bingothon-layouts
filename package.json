{
  "name": "score-layouts",
  "version": "1.0.0",
  "description": "Layouts for Bingothon using the template for NodeCG bundles that use Vue.js and TypeScript.",
  "homepage": "https://github.com/bingothon/bingothon-layouts",
  "bugs": {
    "url": "https://github.com/bingothon/bingothon-layouts/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bingothon/bingothon-layouts.git"
  },
  "license": "MIT",
  "author": "Bingothon",
  "contributors": [
    "lepelog",
    "floha258"
  ],
  "engines": {
    "node": ">=20.x.x"
  },
  "scripts": {
    "autofix": "run-s autofix:*",
    "autofix:browser": "eslint --fix --cache --ext .ts,.vue src/dashboard && eslint --fix --cache --ext .ts,.vue src/graphics && eslint --fix --cache --ext .ts src/browser-util",
    "autofix:extension": "eslint --fix --cache --ext .ts,.js,.html,.d.ts src/extension types",
    "lint": "run-s lint:*",
    "lint:browser": "eslint --cache --ext .ts,.js,.html,.vue src/dashboard src/graphics src/browser-util",
    "lint:extension": "eslint --cache --ext .ts,.js,.html,.d.ts src/extension types",
    "build": "run-s build:*",
    "build:browser": "cross-env NODE_ENV=production webpack",
    "build:extension": "tsc -b tsconfig.extension.json",
    "clean": "trash node_modules/.cache && trash dashboard && trash graphics && trash extension",
    "start": "node ../..",
    "typeschemas": "node script/typeschemas.js",
    "watch": "run-p watch:*",
    "watch:browser": "webpack -w",
    "watch:extension": "tsc -b tsconfig.extension.json -w",
    "tscver": "tsc --version"
  },
  "alias": {
    "vue": "./node_modules/vue/dist/vue.esm.js"
  },
  "nodecg": {
    "compatibleRange": "^2.1.0",
    "dashboardPanels": [
      {
        "name": "Score Control",
        "title": "Score Control",
        "file": "score-control.html",
        "headerColor": "#b92b36",
        "width": 2,
        "workspace": "Main"
      },
      {
        "name": "Countdown Control",
        "title": "Countdown Control",
        "file": "countdown-control.html",
        "headerColor": "#b92b36",
        "width": 2,
        "workspace": "Countdown"
      },
      {
        "name": "Timer Control",
        "title": "Timer Control",
        "file": "timer-control.html",
        "headerColor": "#b92b36",
        "width": 2,
        "workspace": "Main"
      }
    ],
    "graphics": [
      {
        "file": "5p-focus.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "5p-small.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "discussion.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "pictures.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "taskmaster-scorecard.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "countdown.html",
        "width": 1920,
        "height": 1080
      }
    ],
    "mount": [
      {
        "directory": "static",
        "endpoint": "static"
      }
    ]
  },
  "vue": {
    "runtimeCompiler": true
  },
  "dependencies": {
    "@discordjs/voice": "^0.16.0",
    "@fortawesome/fontawesome-svg-core": "^6.3.0",
    "@fortawesome/free-solid-svg-icons": "^6.3.0",
    "@fortawesome/vue-fontawesome": "^2.0.10",
    "@mdi/font": "^7.1.96",
    "axios": "^1.5.1",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^5.0.0",
    "deep-equal": "^2.2.0",
    "discord.js": "^13.16.0",
    "gsap": "^3.12.2",
    "module-alias": "^2.2.3",
    "request-promise": "^4.2.4",
    "sass": "~1.32",
    "sass-loader": "^13.0.0",
    "ts-loader": "^8.0.7",
    "vuetify": "^2.7.1",
    "vuetify-loader": "^1.9.2",
    "vuex": "^3.6.2",
    "webpack-livereload-plugin": "^2.3.0",
    "ws": "^8.12.1"
  },
  "devDependencies": {
    "@mdi/font": "^5.4.55",
    "@nodecg/types": "^2.1.12",
    "@types/clone": "^2.1.1",
    "@types/deep-equal": "^1.0.1",
    "@types/node": "^20.12.10",
    "@types/request-promise": "^4.1.44",
    "@types/webpack-env": "^1.17.0",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "@vue/component-compiler-utils": "^3.3.0",
    "clone": "^2.1.2",
    "cross-env": "^7.0.2",
    "css-loader": "^6.7.1",
    "deepmerge": "^4.2.2",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.7.0",
    "file-loader": "^6.0.0",
    "fork-ts-checker-webpack-plugin": "^7.2.11",
    "globby": "^11.1.0",
    "html-webpack-plugin": "^5.5.0",
    "http-proxy-middleware": "^2.0.6",
    "json-schema-to-typescript": "11.0.2",
    "mini-css-extract-plugin": "^2.7.2",
    "moment": "^2.29.1",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.7.1",
    "svg-inline-loader": "^0.8.2",
    "trash-cli": "^5.0.0",
    "ts-loader": "^9.4.2",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "tslib": "^2.0.0",
    "typescript": "^4.7.3",
    "vue": "^2.7.14",
    "vue-class-component": "^7.2.5",
    "vue-hot-reload-api": "^2.3.4",
    "vue-loader": "^15.10.1",
    "vue-property-decorator": "^9.0.0",
    "vue-router": "^3.6.5",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "vuetify": "^2.6.14",
    "vuetify-loader": "^1.6.0",
    "vuex": "^3.1.1",
    "vuex-class": "^0.3.2",
    "vuex-class-state2way": "^1.0.1",
    "webpack": "^5.73.0",
    "webpack-cli": "^5.0.1",
    "webpack-livereload-plugin": "^3.0.2"
  }
}
